<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="couchbaselabs"
           fetch="git://github.com/couchbaselabs/"
           review="review.couchbase.org" />

  <remote  name="couchbase"
           fetch="git://github.com/couchbase/"
           review="review.couchbase.org" />

  <default revision="master" remote="couchbaselabs" />

  <project path="vendor/couchdb" name="couchdb" remote="couchbase"
           revision="emonk-preview-ios"/>

  <project path="CouchbaseMobile" name="iOS-Couchbase" remote="couchbase"/>

  <project path="CouchCocoa" name="CouchCocoa"/>

  <project path="vendor/iMonkey" name="iMonkey"/>
  <project path="vendor/iErl14" name="iErl" remote="couchbase" />
  <project path="vendor/ios-openssl" name="ios-openssl"/>

</manifest>
